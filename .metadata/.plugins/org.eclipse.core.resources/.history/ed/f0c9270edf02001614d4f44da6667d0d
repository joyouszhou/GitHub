package com.zhouhuan.common.custom.base;

public class ErrorCode {
	/**
	 * 服务器级别
	 */
	public final static int SYSTEM_ERROR 				= 10001; 					// 服务器异常
	public final static int SYSTEM_REQUEST_TIMEOUT 		= 10002; 					// 请求超时
	public final static int SYSTEM_SYNC_APP_ERROR 		= 10003; 					// app同步失败
	
	/**
	 * 验证级别
	 */
	public final static int METHOD_ARGUMENT_NOTVALID 	= 30001; 					// MethodArgumentNotValidException
	public static final int NO_PERMISSION 				= 30002; 					// 无权限访问，签名验证失败
	public final static int HEADER_NOT_EMPTY 			= 30003; 					// http请求头里参数不能为空
	
	

}

package com.zhgame.common.custom.base;

import com.alibaba.fastjson.JSONObject;

public class EntityResponse {

	private int status;
	private String message;
	private String extend1;
	
	public EntityResponse(int status,String message){
		this.message = message;
		this.status = status;
	}
	public EntityResponse(int status,String message,String extend1){
		this.message = message;
		this.status = status;
		this.extend1 = extend1;
	}
	
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	public String getExtend1() {
		return extend1;
	}
	public void setExtend1(String extend1) {
		this.extend1 = extend1;
	}
	
	public static void main(String[] args) {
		EntityResponse success = new EntityResponse(1,"hahaha");
		EntityResponse success1 = new EntityResponse(1,"hahaha","extend");
		System.out.println(JSONObject.toJSON(success));
		System.out.println(JSONObject.toJSON(success1));
	}
	
}

package com.zhouhuan.common.custom.base;

public class ErrorCode {
	
	
	public final static int SYSTEM_ERROR 				= 10000; // 服务器异常
	public static final int NO_PERMISSION 				= 10001; 					// 无权限访问，签名验证失败
	public final static int METHOD_ARGUMENT_NOTVALID 	= 10002; 					// MethodArgumentNotValidException


}
